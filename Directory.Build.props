<Project>
    <PropertyGroup>
        <Authors>DrBarnabus and Contributors</Authors>
        <PackageProjectUrl>https://github.com/DrBarnabus/DacTools</PackageProjectUrl>
        <PackageRequiresLicenseAcceptance>false</PackageRequiresLicenseAcceptance>
        <Copyright>Copyright DrBarnabus 2020.</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Company>$(Authors)</Company>

        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <WarningsAsErrors />
    </PropertyGroup>

    <PropertyGroup>
        <PackageVersion_MicrosoftTestSdk>16.4.0</PackageVersion_MicrosoftTestSdk>
        <PackageVersion_Shouldly>3.0.2</PackageVersion_Shouldly>
        <PackageVersion_Moq>4.13.1</PackageVersion_Moq>
        <PackageVersion_XUnit>2.4.1</PackageVersion_XUnit>
        <PackageVersion_XUnitRunnerVisualStudio>2.4.1</PackageVersion_XUnitRunnerVisualStudio>
        <PackageVersion_CoverletMsBuild>2.7.0</PackageVersion_CoverletMsBuild>
    </PropertyGroup>
</Project>
